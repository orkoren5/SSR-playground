{
    "compilerOptions": {
        "module": "UMD",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "moduleResolution": "node",
        "target": "es6",
        "rootDir": "./src",
        "outDir": "./dist/server",
        "lib": ["dom", "es6", "es2017", "es2017.object", "esnext.asynciterable"]
    },
    "include": ["src/server/server.js"],
    "exclude": ["node_modules"]
}
